import "bling"

return
  <call>: (...) => @.new(...)
  new: (command, instance_name) ->
    bling.module.scratchpad
      command: command
      rule:
        instance: instance_name
      sticky: true
      autoclose: true
      floating: true
      reapply: true
      dont_focus_before_close: false
      geometry:
        x: 360
        y: 90
        height: 900
        width: 1200
